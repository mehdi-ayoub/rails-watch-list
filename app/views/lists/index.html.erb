

<div class="background-image">
  <%= image_tag 'movies.jpeg', alt: 'Movies' %>
</div>

<div class="mt-3 list-group">
  <ul class="list-group">
    <% @lists.each do |list| %>
      <li class="list-group-item list-group-item-action" aria-current="true"><%= link_to list.name, list_path(list) %></li>
    <% end %>
  </ul>
</div>
